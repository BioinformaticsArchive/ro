###  http://purl.org/wf4ever/ro#AggregatedAnnotation

ro:AggregatedAnnotation rdf:type owl:Class ;
                        
                        rdfs:subClassOf [ rdf:type owl:Restriction ;
                                          owl:onProperty ao:annotatesResource ;
                                          owl:someValuesFrom [ rdf:type owl:Class ;
                                                               owl:unionOf ( roevo:Change
                                                                             roevo:ChangeSpecification
                                                                             roevo:VersionableResource
                                                                           )
                                                             ]
                                        ] .



###  http://purl.org/wf4ever/ro#ResearchObject

ro:ResearchObject rdf:type owl:Class .



###  http://purl.org/wf4ever/ro#Resource

ro:Resource rdf:type owl:Class .



###  http://purl.org/wf4ever/roevo#Addition

roevo:Addition rdf:type owl:Class ;
               
               rdfs:subClassOf roevo:Change .



###  http://purl.org/wf4ever/roevo#ArchivedRO

roevo:ArchivedRO rdf:type owl:Class ;
                 
                 rdfs:subClassOf ro:ResearchObject ,
                                 prov:Entity .



###  http://purl.org/wf4ever/roevo#Change

roevo:Change rdf:type owl:Class ;
             
             rdfs:subClassOf prov:Activity .



###  http://purl.org/wf4ever/roevo#ChangeSpecification

roevo:ChangeSpecification rdf:type owl:Class ;
                          
                          rdfs:subClassOf prov:Activity .



###  http://purl.org/wf4ever/roevo#LiveRO

roevo:LiveRO rdf:type owl:Class ;
             
             rdfs:subClassOf ro:ResearchObject ,
                             prov:Entity .



###  http://purl.org/wf4ever/roevo#Modification

roevo:Modification rdf:type owl:Class ;
                   
                   rdfs:subClassOf roevo:Change .



###  http://purl.org/wf4ever/roevo#Removal

roevo:Removal rdf:type owl:Class ;
              
              rdfs:subClassOf roevo:Change .



###  http://purl.org/wf4ever/roevo#SnapshotRO

roevo:SnapshotRO rdf:type owl:Class ;
                 
                 rdfs:subClassOf ro:ResearchObject ,
                                 prov:Entity .



###  http://purl.org/wf4ever/roevo#VersionableResource

roevo:VersionableResource rdf:type owl:Class ;
                          
                          owl:equivalentClass [ rdf:type owl:Class ;
                                                owl:unionOf ( ro:AggregatedAnnotation
                                                              ro:Resource
                                                              roevo:ArchivedRO
                                                              roevo:SnapshotRO
                                                            )
                                              ] ;
                          
                          rdfs:subClassOf prov:Entity ,
                                          [ rdf:type owl:Restriction ;
                                            owl:onProperty dcterms:identifier ;
                                            owl:someValuesFrom rdfs:Literal
                                          ] .



###  http://www.w3.org/ns/prov#Activity

prov:Activity rdf:type owl:Class .



###  http://www.w3.org/ns/prov#Agent

prov:Agent rdf:type owl:Class .



###  http://www.w3.org/ns/prov#Entity

prov:Entity rdf:type owl:Class .




