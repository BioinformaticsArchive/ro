###  http://purl.org/ao/annotatesResource

ao:annotatesResource rdf:type owl:ObjectProperty .



###  http://purl.org/wf4ever/roevo#fromVersion

roevo:fromVersion rdf:type owl:ObjectProperty ;
                  
                  rdfs:domain roevo:ChangeSpecification ;
                  
                  rdfs:range roevo:VersionableResource .



###  http://purl.org/wf4ever/roevo#hasArchive

roevo:hasArchive rdf:type owl:ObjectProperty ;
                 
                 rdfs:range roevo:ArchivedRO ;
                 
                 rdfs:domain roevo:LiveRO ;
                 
                 owl:inverseOf roevo:isArchiveOf .



###  http://purl.org/wf4ever/roevo#hasChange

roevo:hasChange rdf:type owl:ObjectProperty ;
                
                rdfs:range roevo:Change ;
                
                rdfs:domain roevo:ChangeSpecification .



###  http://purl.org/wf4ever/roevo#hasPreviousChange

roevo:hasPreviousChange rdf:type owl:ObjectProperty ;
                        
                        rdfs:range roevo:Change ;
                        
                        rdfs:domain roevo:Change .



###  http://purl.org/wf4ever/roevo#hasRevision

roevo:hasRevision rdf:type owl:ObjectProperty .



###  http://purl.org/wf4ever/roevo#hasSnapshot

roevo:hasSnapshot rdf:type owl:ObjectProperty ;
                  
                  rdfs:domain roevo:LiveRO ;
                  
                  rdfs:range roevo:SnapshotRO ;
                  
                  owl:inverseOf roevo:isSnapshotOf .



###  http://purl.org/wf4ever/roevo#isArchiveOf

roevo:isArchiveOf rdf:type owl:ObjectProperty .



###  http://purl.org/wf4ever/roevo#isSnapshotOf

roevo:isSnapshotOf rdf:type owl:ObjectProperty .



###  http://purl.org/wf4ever/roevo#relatedResource

roevo:relatedResource rdf:type owl:ObjectProperty ;
                      
                      rdfs:domain roevo:Change ;
                      
                      rdfs:range roevo:VersionableResource ;
                      
                      rdfs:subPropertyOf prov:used .



###  http://purl.org/wf4ever/roevo#toVersion

roevo:toVersion rdf:type owl:ObjectProperty ;
                
                rdfs:domain roevo:ChangeSpecification ;
                
                rdfs:range roevo:VersionableResource .



###  http://purl.org/wf4ever/roevo#wasArchivedBy

roevo:wasArchivedBy rdf:type owl:ObjectProperty ;
                    
                    rdfs:domain roevo:ArchivedRO ;
                    
                    rdfs:range prov:Agent ;
                    
                    rdfs:subPropertyOf prov:wasAttributedTo .



###  http://purl.org/wf4ever/roevo#wasChangedBy

roevo:wasChangedBy rdf:type owl:ObjectProperty ;
                   
                   rdfs:range roevo:ChangeSpecification ;
                   
                   rdfs:domain roevo:VersionableResource ;
                   
                   rdfs:subPropertyOf prov:wasGeneratedBy .



###  http://purl.org/wf4ever/roevo#wasSnapshotedBy

roevo:wasSnapshotedBy rdf:type owl:ObjectProperty ;
                      
                      rdfs:domain roevo:SnapshotRO ;
                      
                      rdfs:range prov:Agent ;
                      
                      rdfs:subPropertyOf prov:wasAttributedTo .



###  http://www.w3.org/ns/prov#hadOriginalSource

prov:hadOriginalSource rdf:type owl:ObjectProperty ;
                      
                      rdfs:range prov:Entity ;
                      
                      rdfs:domain prov:Entity ;
                      
                      rdfs:subPropertyOf prov:wasDerivedFrom .



###  http://www.w3.org/ns/prov#used

prov:used rdf:type owl:ObjectProperty .



###  http://www.w3.org/ns/prov#wasAssociatedWith

prov:wasAssociatedWith rdf:type owl:ObjectProperty ;
                       
                       rdfs:domain prov:Activity ;
                       
                       rdfs:range prov:Agent .



###  http://www.w3.org/ns/prov#wasAttributedTo

prov:wasAttributedTo rdf:type owl:ObjectProperty .



###  http://www.w3.org/ns/prov#wasDerivedFrom

prov:wasDerivedFrom rdf:type owl:AsymmetricProperty ,
                             owl:IrreflexiveProperty ,
                             owl:ObjectProperty ;
                    
                    rdfs:range prov:Entity ;
                    
                    rdfs:domain prov:Entity .



###  http://www.w3.org/ns/prov#wasGeneratedBy

prov:wasGeneratedBy rdf:type owl:ObjectProperty .



###  http://www.w3.org/ns/prov#wasQuotedFrom

prov:wasQuotedFrom rdf:type owl:ObjectProperty ;
                   
                   rdfs:domain prov:Entity ;
                   
                   rdfs:range prov:Entity ;
                   
                   rdfs:subPropertyOf prov:wasDerivedFrom .



###  http://www.w3.org/ns/prov#wasRevisionOf

prov:wasRevisionOf rdf:type owl:ObjectProperty ;
                   
                   owl:inverseOf roevo:hasRevision ;
                   
                   rdfs:range prov:Entity ;
                   
                   rdfs:domain prov:Entity ;
                   
                   rdfs:subPropertyOf prov:wasDerivedFrom .





