###  http://purl.org/dc/terms/identifier

dcterms:identifier rdf:type owl:DatatypeProperty .



###  http://purl.org/wf4ever/roevo#archivedAtTime

roevo:archivedAtTime rdf:type owl:DatatypeProperty ;
                     
                     rdfs:domain roevo:ArchivedRO ;
                     
                     rdfs:range xsd:dateTime ;
                     
                     rdfs:subPropertyOf prov:generatedAtTime .



###  http://purl.org/wf4ever/roevo#snapshotedAtTime

roevo:snapshotedAtTime rdf:type owl:DatatypeProperty ;
                       
                       rdfs:domain roevo:SnapshotRO ;
                       
                       rdfs:range xsd:dateTime ;
                       
                       rdfs:subPropertyOf prov:generatedAtTime .



###  http://www.w3.org/ns/prov#endedAtTime

prov:endedAtTime rdf:type owl:DatatypeProperty ,
                          owl:FunctionalProperty ;
                 
                 rdfs:range xsd:dateTime ;
                 
                 rdfs:domain prov:Activity .



###  http://www.w3.org/ns/prov#generatedAtTime

prov:generatedAtTime rdf:type owl:DatatypeProperty .



###  http://www.w3.org/ns/prov#startedAtTime

prov:startedAtTime rdf:type owl:DatatypeProperty ,
                            owl:FunctionalProperty ;
                   
                   rdfs:range xsd:dateTime ;
                   
                   rdfs:domain prov:Activity .





