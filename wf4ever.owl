<rdf:RDF xmlns="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">

    <owl:Ontology rdf:about="http://purl.org/wf4ever/wf4ever">
        <comment xml:lang="en">The wf4ever ontology.
                                  This is based on the wf4ever ontologies ro, wf and wfdesc- but adds custom classes like wf4ever:Image and wf4ever:WebServiceProcess</comment>
        <owl:imports rdf:resource="http://purl.org/wf4ever/ro"/>
        <owl:imports rdf:resource="http://purl.org/wf4ever/wfdesc"/>
        <owl:imports rdf:resource="http://purl.org/wf4ever/wfprov"/>
        <owl:versionIRI rdf:resource="https://raw.github.com/wf4ever/ro/master/wf4ever.owl"/>
    </owl:Ontology>

    <owl:Class rdf:about="http://purl.org/wf4ever/wf4ever#BeanshellScript">
        <subClassOf rdf:resource="http://purl.org/wf4ever/wf4ever#Script"/>
    </owl:Class>

    <owl:Class rdf:about="http://purl.org/wf4ever/wf4ever#CommandLineTool">
        <subClassOf rdf:resource="http://purl.org/wf4ever/wfdesc#Process"/>
    </owl:Class>

    <owl:Class rdf:about="http://purl.org/wf4ever/wf4ever#Dataset">
        <subClassOf rdf:resource="http://purl.org/wf4ever/wfprov#Artifact"/>
    </owl:Class>

    <owl:Class rdf:about="http://purl.org/wf4ever/wf4ever#Document">
        <subClassOf rdf:resource="http://purl.org/wf4ever/wfprov#Artifact"/>
    </owl:Class>

    <owl:Class rdf:about="http://purl.org/wf4ever/wf4ever#File">
        <subClassOf rdf:resource="http://purl.org/wf4ever/wfprov#Artifact"/>
    </owl:Class>

    <owl:Class rdf:about="http://purl.org/wf4ever/wf4ever#Image">
        <subClassOf rdf:resource="http://purl.org/wf4ever/wfprov#Artifact"/>
    </owl:Class>

    <owl:Class rdf:about="http://purl.org/wf4ever/wf4ever#RESTService">
        <comment xml:lang="en">A RESTful web service</comment>
        <subClassOf rdf:resource="http://purl.org/wf4ever/wf4ever#WebService"/>
    </owl:Class>

    <owl:Class rdf:about="http://purl.org/wf4ever/wf4ever#RScript">
        <subClassOf rdf:resource="http://purl.org/wf4ever/wf4ever#Script"/>
    </owl:Class>

    <owl:Class rdf:about="http://purl.org/wf4ever/wf4ever#SOAPService">
        <comment xml:lang="en">A SOAP service is typically described in a WSDL 1.1 or WSDL 2.0

TODO: Do we need to distinguish between WSDL and SOAP? WSDL 1.1 and WSDL 2.0? RPC/Encoded vs.Wrapped Document/Literal?</comment>
        <subClassOf rdf:resource="http://purl.org/wf4ever/wf4ever#WebService"/>
    </owl:Class>

    <owl:Class rdf:about="http://purl.org/wf4ever/wf4ever#Script">
        <subClassOf rdf:resource="http://purl.org/wf4ever/wfdesc#Process"/>
    </owl:Class>

    <owl:Class rdf:about="http://purl.org/wf4ever/wf4ever#WebService">
        <comment>WebServiceProcess is a wfdesc:Process description, the enactment of which gives rise to a web service call.</comment>
        <subClassOf rdf:resource="http://purl.org/wf4ever/wfdesc#Process"/>
    </owl:Class>

    <owl:Class rdf:about="http://purl.org/wf4ever/wf4ever#WorkflowResearchObject">
        <comment>A workflow research object is a research object that contains at least one workflow description.</comment>
    </owl:Class>

    <owl:DatatypeProperty rdf:about="http://purl.org/wf4ever/wf4ever#command">
        <domain rdf:resource="http://purl.org/wf4ever/wf4ever#CommandLineTool"/>
        <range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="http://purl.org/wf4ever/wf4ever#script">
        <domain rdf:resource="http://purl.org/wf4ever/wf4ever#Script"/>
        <range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="http://purl.org/wf4ever/wf4ever#serviceURI">
        <domain rdf:resource="http://purl.org/wf4ever/wf4ever#WebService"/>
        <range rdf:resource="http://www.w3.org/2001/XMLSchema#anyURI"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="http://purl.org/wf4ever/wf4ever#wsdlOperationName">
        <domain rdf:resource="http://purl.org/wf4ever/wf4ever#SOAPService"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="http://purl.org/wf4ever/wf4ever#wsdlPortName">
        <domain rdf:resource="http://purl.org/wf4ever/wf4ever#SOAPService"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="http://purl.org/wf4ever/wf4ever#wsdlURI">
        <domain rdf:resource="http://purl.org/wf4ever/wf4ever#SOAPService"/>
        <subPropertyOf rdf:resource="http://purl.org/wf4ever/wf4ever#serviceURI"/>
    </owl:DatatypeProperty>
</rdf:RDF>
