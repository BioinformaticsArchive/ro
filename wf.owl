###  http://www.wf4ever-project.org/vocab/wf#hasProcessTemplate

:hasProcessTemplate rdf:type owl:ObjectProperty ;
                    
                    rdfs:comment "This object property is used to specify that the given workflow template contains the given process template as part of its definition."@en ;
                    
                    rdfs:range :ProcessTemplate ;
                    
                    rdfs:domain :WorkflowTemplate .



###  http://www.wf4ever-project.org/vocab/wf#hasSubWorkflowTemplate

:hasSubWorkflowTemplate rdf:type owl:ObjectProperty ;
                        
                        rdfs:comment """This object property is used to associate a workflow template to another workflow template, specifying that the given workflow has the given sub-workflow as a contained process. 

This is a specialisation of hasProcessTemplate."""@en ;
                        
                        rdfs:range :WorkflowTemplate ;
                        
                        rdfs:domain :WorkflowTemplate ;
                        
                        rdfs:subPropertyOf :hasProcessTemplate .





