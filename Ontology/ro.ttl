###  http://www.wf4ever-project.org/vocab/ro#contains

:contains rdf:type owl:ObjectProperty ;
          
          rdfs:comment "This property associates a workflow run with a process to specify that the latter is a constituent of the former." ;
          
          rdfs:range <http://purl.org/net/opmv/ns#Process> ;
          
          rdfs:subPropertyOf owl:topObjectProperty ;
          
          rdfs:domain :WorkflowRun .



###  http://www.wf4ever-project.org/vocab/ro#hasArtifactTemplate

:hasArtifactTemplate rdf:type owl:ObjectProperty ;
                     
                     rdfs:comment "This object property is used to associate an artifact to its correponding template." ;
                     
                     rdfs:domain <http://purl.org/net/opmv/ns#Artifact> ;
                     
                     rdfs:subPropertyOf owl:topObjectProperty ;
                     
                     rdfs:range :ArtifactTemplate .



###  http://www.wf4ever-project.org/vocab/ro#hasInput

:hasInput rdf:type owl:ObjectProperty ;
          
          rdfs:comment "This object property is used to specify the template of the artifacts used as input to a given process template." ;
          
          rdfs:range :ArtifactTemplate ;
          
          rdfs:domain :ProcessTemplate .



###  http://www.wf4ever-project.org/vocab/ro#hasOutput

:hasOutput rdf:type owl:ObjectProperty ;
           
           rdfs:comment "This object property is used to specify the template of the artifacts used as output to a given process template." ;
           
           rdfs:range :ArtifactTemplate ;
           
           rdfs:domain :ProcessTemplate .



###  http://www.wf4ever-project.org/vocab/ro#hasProcessTemplate

:hasProcessTemplate rdf:type owl:ObjectProperty ;
                    
                    rdfs:comment "This object property associate a process to its template." ;
                    
                    rdfs:domain <http://purl.org/net/opmv/ns#Process> ;
                    
                    rdfs:subPropertyOf owl:topObjectProperty ;
                    
                    rdfs:range :ProcessTemplate .



###  http://www.wf4ever-project.org/vocab/ro#hasWorkflowTemplate

:hasWorkflowTemplate rdf:type owl:ObjectProperty ;
                     
                     rdfs:comment "This property associates a workflow run to its corresponding workflow template." ;
                     
                     rdfs:domain :WorkflowRun ;
                     
                     rdfs:range :WorkflowTemplate ;
                     
                     rdfs:subPropertyOf :hasProcessTemplate .



###  http://www.wf4ever-project.org/vocab/ro#isPartOf

:isPartOf rdf:type owl:ObjectProperty ;
          
          rdfs:comment "This object property is used to specify that a given process template is part of a given workflow template."^^rdf:PlainLiteral ;
          
          rdfs:subPropertyOf owl:topObjectProperty ;
          
          rdfs:domain :ProcessTemplate ;
          
          rdfs:range :WorkflowTemplate .



###  http://www.wf4ever-project.org/vocab/ro#isSubWorkflowOf

:isSubWorkflowOf rdf:type owl:ObjectProperty ;
                 
                 rdfs:comment "This object property is used to associate a workflow template to another workflow template, specifying that the former is a sub-workflow of the latter." ;
                 
                 rdfs:range :WorkflowTemplate ;
                 
                 rdfs:domain :WorkflowTemplate ;
                 
                 rdfs:subPropertyOf :isPartOf .



###  http://www.wf4ever-project.org/vocab/ro#wasEnactedBy

:wasEnactedBy rdf:type owl:ObjectProperty ;
              
              rdfs:comment "wasEnactedBy associates a process with a workflow engine, specifying that the execution of the former was enacted by the latter." ;
              
              rdfs:subPropertyOf <http://purl.org/net/opmv/ns#wasControlledBy> ;
              
              rdfs:range [ rdf:type owl:Restriction ;
                           owl:onProperty :wasEnactedBy ;
                           owl:someValuesFrom :WorkflowEngine
                         ] ;
              
              rdfs:domain [ rdf:type owl:Restriction ;
                            owl:onProperty :wasEnactedBy ;
                            owl:someValuesFrom <http://purl.org/net/opmv/ns#Process>
                          ] .



###  http://www.wf4ever-project.org/vocab/ro#wasSpecifiedUsing

:wasSpecifiedUsing rdf:type owl:ObjectProperty ;
                   
                   rdfs:comment "This object property is used to specify the workflow workbench using which a given workflow template was specified." ;
                   
                   rdfs:subPropertyOf owl:topObjectProperty ;
                   
                   rdfs:domain :WorkflowTemplate ;
                   
                   rdfs:range :WorkflowWorkbench .





