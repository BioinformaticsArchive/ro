:instanceOfArtifactTemplate rdf:type owl:ObjectProperty ;
                     
                     rdfs:comment "This object property is used to associate an artifact to its correponding template."@en ;
                     
                     rdfs:domain opm:Artifact ;
                     
                     rdfs:range wf:ArtifactTemplate .



:instanceOfProcessTemplate rdf:type owl:ObjectProperty ;
                    
                    rdfs:comment "This object property associate a process to its template."@en ;
                    
                    rdfs:domain opm:Process ;
                    
                    rdfs:range wf:ProcessTemplate .



:instanceOfWorkflowTemplate rdf:type owl:ObjectProperty ;
                     
                     rdfs:comment "This property associates a workflow run to its corresponding workflow template."@en ;
                     
                     rdfs:domain :WorkflowRun ;
                     
                     rdfs:subPropertyOf :instanceOfProcessTemplate;
                     
                     rdfs:range wf:WorkflowTemplate .



###  http://www.wf4ever-project.org/vocab/wfprov#wasEnactedBy

:wasEnactedBy rdf:type owl:ObjectProperty ;
              
              rdfs:comment "wasEnactedBy associates a process with a workflow engine, specifying that the execution of the former was enacted by the latter."@en ;
              
              rdfs:subPropertyOf opm:wasControlledBy ;
              
              rdfs:range :WorkflowEngine .



###  http://www.wf4ever-project.org/vocab/wfprov#wasPartOfWorkflowRun

:wasPartOfWorkflowRun rdf:type owl:ObjectProperty ;
                      
                      rdfs:comment "This property associates a workflow run with a process to specify that the latter is a constituent of the former."@en ;
                      rdfs:domain opm:Process ; 
                      rdfs:range :WorkflowRun .



###  http://www.wf4ever-project.org/vocab/wfprov#wasSpecifiedUsing

:wasSpecifiedUsing rdf:type owl:ObjectProperty ;
                   
                   rdfs:comment "This object property is used to specify the workflow workbench using which a given workflow template was specified."@en ;
                   
                   rdfs:subPropertyOf owl:topObjectProperty ;
                   
                   rdfs:range :WorkflowEditor ;
                   
                   rdfs:domain wf:WorkflowTemplate .





