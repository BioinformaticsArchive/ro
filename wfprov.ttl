###  http://www.wf4ever-project.org/vocab/wfprov#Tool

:Tool rdf:type owl:Class ;
      
      rdfs:subClassOf opm:Agent ;
      
      rdfs:comment "A software program (which can act as an opm:Agent/foaf:Agent)"@en .



###  http://www.wf4ever-project.org/vocab/wfprov#WorkflowEditor

:WorkflowEditor rdf:type owl:Class ;
                
                rdfs:subClassOf :Tool ;
                
                rdfs:comment "A workflow editor is a software program that provides designers with a a means to write Workflow Templates."@en ;
                
                rdfs:seeAlso wf:WorkflowTemplate .



###  http://www.wf4ever-project.org/vocab/wfprov#WorkflowEngine

:WorkflowEngine rdf:type owl:Class ;
                
                rdfs:subClassOf :Tool ;
                
                rdfs:comment "A workflow engine is an egent that is responsible for enacting a given workflow template. The result of workflow enactment gives rise to a workflow run."@en ;
                
                rdfs:seeAlso :WorkflowRun .



###  http://www.wf4ever-project.org/vocab/wfprov#WorkflowRun

:WorkflowRun rdf:type owl:Class ;
             
             rdfs:subClassOf opm:Process ;
             
             rdfs:comment "A workflow run is a Process which have been enacted by a Workflow Engine, according to the execution definition from a Workflow Template. Such a process typically contains several subprocesses corresponding to Process Definitions"@en ;
             
             rdfs:seeAlso :WorkflowEngine ,
                          wf:ProcessTemplate ,
                          wf:WorkflowTemplate .




