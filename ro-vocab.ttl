@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

@prefix dc: <http://purl.org/dc/elements/1.1/> .                                                         
@prefix dcterms: <http://purl.org/dc/terms/> .                                                           
@prefix foaf: <http://xmlns.com/foaf/0.1/> .                                                             
@prefix ore: <http://www.openarchives.org/ore/terms/> .  
@prefix oac: <http://www.openannotation.org/ns/> . 

@prefix ro: <http://www.wf4ever-project.org/2011/ro/core/> .

ro: a owl:Ontology ;
    rdfs:label "Research Object Core v1.0" ;
    dc:title "Research Object Core ontology" ;
    dcterms:created "2011-08-12T11:40:43Z"^^xsd:datetime ;
    dcterms:creator _:khalid ;
    dcterms:contributor _:stian .

_:stian a foaf:Person ;
    owl:sameAs <http://www.soiland-reyes.com/stian/#me> ;
    foaf:name "Stian Soiland-Reyes"@en ;
    foaf:mbox <mailto:soiland-reyes@cs.manchester.ac.uk> ;
    foaf:homepage <http://www.soiland-reyes.com/stian/> .

_:khalid a foaf:Person ;
    owl:sameAs <http://semanticweb.org/id/Khalid_Belhajjame> ;
    foaf:name "Khalid Belhajjame"@en ;
    foaf:mbox <mailto:Khalid.Belhajjame@cs.man.ac.uk> ;
    foaf:homepage <http://www.cs.man.ac.uk/%7Ekhalidb> , 
                  <http://semanticweb.org/wiki/Khalid_Belhajjame> .
    

ro:ResearchObject a owl:Class ;
    rdfs:subClassOf ore:Aggregation ;
    rdfs:subClassOf ore:AggregatedResource.

ro:ResearchObject a owl:Class .
ro:Workflow a owl:Class .
ro:WebService a owl:Class .
ro:Document a owl:Class .
ro:Input a owl:Class .
ro:Output a owl:Class .
ro:SoftwareTool a owl:Class .

ro:Manifest a owl:Class ;
    rdfs:subClassOf ore:ResourceMap .
ro:Workflow a owl:Class ;
    rdfs:subClassOf ore:AggregatedResource .
ro:WorkflowRun a owl:Class ;
    rdfs:subClassOf ore:AggregatedResource .
ro:WebService a owl:Class ;
    rdfs:subClassOf ore:AggregatedResource .
ro:Document a owl:Class ;
    rdfs:subClassOf ore:AggregatedResource .
ro:DataSet a owl:Class ;
    rdfs:subClassOf ore:AggregatedResource .
ro:Software a owl:class ;
    rdfs:subClassOf ore:AggregatedResource .

ro:isUsedIn
    a rdf:Property ;
    rdfs:domain ro:WebService .
#    rdfs:range nrl:Semantics .

ro:hasInput
    a rdf:Property ;
    rdfs:domain ro:DataSet ;
    rdfs:range ro:WorkflowRun .

ro:hasOutput
    a rdf:Property ;
    rdfs:domain ro:WorkflowRun ;
    rdfs:range ro:DataSet .

ro:instanceOfWf
    a rdf:Property ;
    rdfs:domain ro:WorkflowRun ;
    rdfs:range ro:Workflow .

ro:subWorkflowOf
    a rdf:Property ;
    rdfs:domain ro:Workflow ;
    rdfs:range ro:Workflow .
