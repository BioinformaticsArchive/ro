    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/co/Collection -->

    <rdf:Description rdf:about="&co;Collection">
        <rdfs:subClassOf rdf:resource="&roterms;WorkflowValue"/>
    </rdf:Description>
    


    <!-- http://purl.org/wf4ever/roterms#OptionalInput -->

    <owl:Class rdf:about="&roterms;OptionalInput">
        <rdfs:label xml:lang="en">Optional input</rdfs:label>
        <rdfs:subClassOf rdf:resource="&wfdesc;Input"/>
        <rdfs:comment xml:lang="en">An input parameter that is optional, meaning a value is not (always) neccessary for correct execution. 

An optional parameter therefore does not need to be connected to a datalink, or it might receive an empty (null-like) value.

The process might internally assume a roterms:defaultParameter if the input is not provided.</rdfs:comment>
        <rdfs:seeAlso rdf:resource="&roterms;defaultValue"/>
    </owl:Class>
    


    <!-- http://purl.org/wf4ever/roterms#WorkflowValue -->

    <owl:Class rdf:about="&roterms;WorkflowValue">
        <rdfs:label xml:lang="en">Workflow value</rdfs:label>
        <rdfs:subClassOf rdf:resource="&prov;Entity"/>
        <rdfs:comment xml:lang="en">A workflow value. The value is a piece of data that was used or generated by a wfdesc:WorkflowRun (as a wfprov:Artifact), or it be a suggested value for a wfdesc:Parameter given by roterms:exampleValue or roterms:defaultValue.

A value can be provided as a resource (just a URI), embedded in RDF as text (cnt:ContentAsText), bytes (cnt:ContentAsBase64), or be a co:Collection of other roterms:WorkflowValue, typically an ordered co:List.</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/wf4ever/wfprov#Artifact -->

    <rdf:Description rdf:about="&wfprov;Artifact">
        <rdfs:subClassOf rdf:resource="&roterms;WorkflowValue"/>
    </rdf:Description>
    


    <!-- http://www.w3.org/2011/content#Content -->

    <rdf:Description rdf:about="&cnt;Content">
        <rdfs:subClassOf rdf:resource="&roterms;WorkflowValue"/>
    </rdf:Description>
</rdf:RDF>



